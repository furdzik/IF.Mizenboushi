{
  "name": "if.mizenboushi",
  "version": "0.0.1",
  "description": "Linters by Izabela Furdzik",
  "main": "index.js",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "bootstrap": "npm i --workspaces",
    "audit": "npm audit --workspaces",
    "outdated": "npm outdated --workspaces",
    "lint": "npm run lint --workspaces",
    "tests": "npm run test --workspaces",
    "publish": "npm publish --workspaces",
    "publish:commitlint": "npm publish --workspace=commitlint-config-kamisama",
    "publish:eslint": "npm publish --workspace=eslint-config-kamisama",
    "publish:tslint": "npm publish --workspace=tslint-config-kamisama",
    "publish:stylelint": "npm publish --workspace=stylelint-config-kamisama"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furdzik/IF.Mizenboushi.git"
  },
  "keywords": [
    "Linters",
    "IF.Kamisama",
    "StyleGuide"
  ],
  "author": "Izabela Furdzik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/furdzik/IF.Mizenboushi/issues"
  },
  "homepage": "https://github.com/furdzik/IF.Mizenboushi#readme",
  "devDependencies": {
    "commitlint-config-kamisama": "file:./packages/commitlint-config",
    "editorconfig": "2.0.0",
    "husky": "8.0.3"
  }
}
