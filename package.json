{
  "name": "if.mizenboushi",
  "version": "0.0.1",
  "description": "Linters by Izabela Furdzik",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "lerna run lint",
    "tests": "lerna run test",
    "release": "lerna publish patch --yes --no-commit-hooks",
    "development-release": "lerna publish --yes --conventional-prerelease --no-changelog --no-commit-hooks --force-publish",
    "bootstrap": "npx lerna exec npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furdzik/IF.Mizenboushi"
  },
  "keywords": [
    "linters",
    "IF.Kamisama",
    "StyleGuide"
  ],
  "author": "Izabela Furdzik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/furdzik/IF.Mizenboushi/issues"
  },
  "homepage": "https://github.com/furdzik/IF.Mizenboushi#readme",
  "devDependencies": {
    "@furdzik/commitlint-config": "file:./packages/commitlint-config",
    "editorconfig": "0.15.3",
    "husky": "7.0.4",
    "lerna": "4.0.0"
  }
}
